
{
    "label" : "lock",
    "action" : "systemctl suspend && swaylock",
    "text" : "suspend",
    "keybind" : "l"
}

{
    "label" : "logout",
    "action" : "hyprctl dispatch exit && swaylock",
    "text" : "Logout",
    "keybind" : "e"
}

{
    "label" : "shutdown",
    "action" : "shutdown now",
    "text" : "Shutdown",
    "keybind" : "s"
}

{
    "label" : "reboot",
    "action" : "reboot",
    "text" : "Reboot",
    "keybind" : "r"
}
